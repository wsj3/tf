{
  "name": "therapists-friend",
  "version": "1.0.0",
  "description": "AI driven tool in support of mental health therapy",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
    "install": "concurrently \"cd frontend && npm install\" \"cd backend && npm install\"",
    "build": "concurrently \"cd frontend && npm run build\" \"cd backend && npm run build\"",
    "test": "concurrently \"cd frontend && npm test\" \"cd backend && npm test\"",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "therapy",
    "mental health",
    "AI"
  ],
  "author": "Your Name",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^6.5.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}